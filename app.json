{
  "expo": {
    "name": "TipToBe",
    "slug": "ttb",
    "version": "1.0.0",
    "orientation": "square",
    "icon": "./assets/icon.jpeg",
   
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true, "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app is able to attach car from your photo library",
        "NSCameraUsageDescription": "This app uses the camera to make a photo of your car"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE", 
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "ttb"
  }
}
